{
  "name": "cinephoria-app",
  "version": "1.0.0",
  "description": "Cinephoria App",
  "main": "backend/server.js",
  "type": "module",
  "scripts": {
    "backend": "node env_scripts/localBackend.js",
    "backend:docker": "node env_scripts/dockerBackend.js",
    "frontend": "npm run dev --prefix frontend",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run backend\" \"npm run frontend\"",
    "prod": "node backend/server.js",
    "build": "echo 'Building frontend' && cd frontend && npm run build",
    "test": "npm run test:backend:unit && npm run test:backend:functional && npm run test:backend:integration && npm run test:frontend:unit && npm run test:frontend:functional && npm run test:frontend:integration",
    "test:backend:unit": "vitest tests/backend/unit --config ./vitest.config.cjs",
    "test:backend:functional": "vitest tests/backend/functional --config ./vitest.config.cjs",
    "test:backend:integration": "vitest tests/backend/integration --config ./vitest.config.cjs",
    "test:backend:unit:local": "node env_scripts/tests/localUnitBckendTests.js",
    "test:backend:functional:local": "node env_scripts/tests/localFuncBckendTests.js",
    "test:backend:integration:local": "node env_scripts/tests/localIntBckendTests.js",
    "test:e2e:open": "cypress open --config-file cypress/cypress.config.js",
    "test:e2e:run": "cypress run --config-file cypress/cypress.config.js",
    "test:frontend:unit": "jest tests/frontend/unit",
    "test:frontend:functional": "jest tests/frontend/functional",
    "test:frontend:integration": "jest tests/frontend/integration",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "add-admin": "node env_scripts/runAddAdmin.js",
    "seed": "node prisma/seed.js",
    "reset-db": "npx prisma migrate reset --force && npm run seed",
    "start": "node backend/server.js"
  },
  "keywords": [],
  "author": "Jeremy Lhomme",
  "license": "ISC",
